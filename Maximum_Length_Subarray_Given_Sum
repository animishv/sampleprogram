public static int maximumLengthSubarrayToGivenSum(int[] input, int K){
int count = 0;
int maxLength = 0;

for(int i = 0; i < input.length; i++){
int sum = 0;
for(int j = i; j < input.length; j++){
sum += input[j];
if(sum == K){
if(j - i + 1 > maxLength){
maxLength = j - i + 1;
}
}
}
}
return maxLength;
}
