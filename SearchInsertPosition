public static int findPosition(int[] input, int target){
	if(input.length == 0 || input = null){
		return -1;
	}
	
	int start = 0;
	int end = input.length - 1;
	int answer = input.length;
	
	
	while(start <= end){
		int mid = (start + end)/2;
		} if(target <= input[mid]){
			answer = mid;
			end = mid - 1;
		} else {
			start = mid + 1;
		}
	}
	return answer;
}
