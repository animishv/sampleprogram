public static TreeNode mergeBST(TreeNode root1, TreeNode root2){
	if(root1 == null && root2 == null){
		return null;
	}
	
	if(root1 == null){
		return root2;
	}
	
	if(root2 == null){
		return root1;
	}
	
	root1.value += root2.value;
	
	root1.left = mergeBST(root1.left, root2.left);
	root2.right = mergeBST(root2.right, root2.right);
	
	return root1;
}
