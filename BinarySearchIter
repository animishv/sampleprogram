public static int binarySearchIter(int[] input, int N) {

        int start = 0;
        int end = input.length - 1;

        while (start < end) {
            int mid = (start + end) / 2;

            if (input[mid] == x) {
                return mid;
            } else if (x > input[mid]) {
                start = mid + 1;
            } else {
                end = mid - 1;
            }

            return -1;
        }
